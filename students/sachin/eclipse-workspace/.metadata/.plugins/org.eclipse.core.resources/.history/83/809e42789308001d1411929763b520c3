
public class PrintEvenNumber100 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int num=2;
		for(int i=0;i<10;i++) {
			for(int j=0;j<5;j++) {
				System.out.print(num+" ");
				num+=2;
			}
			System.out.println();
		}

	}

}
		
		int oddNum=1;
		for(int i=0;i<10;i++) {
			for(int j=0;j<5;j++) {
				System.out.print(oddNum+" ");
				oddNum+=2;
			}
			System.out.println();
		}

	}

}
