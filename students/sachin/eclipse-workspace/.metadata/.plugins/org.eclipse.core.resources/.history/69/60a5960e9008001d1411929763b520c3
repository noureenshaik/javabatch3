
public class PrintSquare {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i=0;i<5;i++) {
			for(int j = 0;j<5;j++){
				if (j==0||j==4||i==0||i==4)
				System.out.print("@");
				else
					System.out.print(" ");
			}
			System.out.println();
		}
		
		for (int x=0;x<5;x++) {
			for (int y=0;y<8;y++) {
				if (x==0||x==4||y==0||y==7)
					System.out.print("#");
				else 
					System.out.print(" ");
			}
			System.out.println();
		}

	}
}


