package PatternProgramming;

public class PyramidProgram {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		for (int i=5;i>0;i--) {
			for (int j=5;j>i;j--) {
				if (j==0)
					System.out.print(" ");
				System.out.print("*");
			}
			System.out.println();
		}
	}

}
