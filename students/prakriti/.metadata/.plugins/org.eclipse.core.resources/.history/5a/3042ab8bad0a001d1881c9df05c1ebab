package assignment2;

public class evenList8 {
	void isEven(){
		 for(int i=1; i<=100;i++){
			if(i%2==0){
		   System.out.println("list of even numbers:");
           System.out.println(i);
			}
	    }
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		evenList8 eo = new evenList8();
		eo.isEven();

	}
}
